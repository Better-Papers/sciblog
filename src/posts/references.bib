@article{teschendorffStatisticalMechanics2021,
  title = {Statistical Mechanics Meets Single-Cell Biology},
  author = {Teschendorff, Andrew E. and Feinberg, Andrew P.},
  year = {2021},
  month = jul,
  journal = {Nat Rev Genet},
  volume = {22},
