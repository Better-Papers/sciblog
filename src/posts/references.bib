}

@article{mairTargetedMultiomic2020,
  title = {A {{Targeted Multi-omic Analysis Approach Measures Protein Expression}} and {{Low-Abundance Transcripts}} on the {{Single-Cell Level}}},
  author = {Mair, Florian and Erickson, Jami R. and Voillet, Valentin and Simoni, Yannick and Bi, Timothy and Tyznik, Aaron J. and Martin, Jody and Gottardo, Raphael and Newell, Evan W. and Prlic, Martin},
  year = {2020},
