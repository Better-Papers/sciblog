  author = {McConnell, S. K. and Kaznowski, C. E.},
  year = {1991},
  month = oct,
  journal = {Science},
  volume = {254},
  number = {5029},
  pages = {282--285},
  issn = {0036-8075},
  doi = {10.1126/science.254.5029.282},
  abstract = {The neocortex is patterned in layers of neurons that are generated in an orderly sequence during development. This correlation between cell birthday and laminar fate prompted an examination of how neuronal phenotypes are determined in the developing cortex. At various times after labeling with [3H]thymidine, embryonic progenitor cells were transplanted into older host brains. The laminar fate of transplanted neurons correlates with the position of their progenitors in the cell cycle at the time of transplantation. Daughters of cells transplanted in S-phase migrate to layer 2/3, as do host neurons. Progenitors transplanted later in the cell cycle, however, produce daughters that are committed to their normal, deep-layer fates. Thus, environmental factors are important determinants of laminar fate, but embryonic progenitors undergo cyclical changes in their ability to respond to such cues.},
  langid = {english},
  pmid = {1925583},
  keywords = {Animals,Brain Tissue Transplantation,Cell Cycle,Cerebral Cortex,Ferrets,G1 Phase,G2 Phase,Mitosis,Neurons,S Phase,Stem Cells}
}

