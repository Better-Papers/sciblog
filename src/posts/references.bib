  shorttitle = {``{{Putting}} on {{My Best Normal}}''},
  author = {Hull, Laura and Petrides, K. V. and Allison, Carrie and Smith, Paula and {Baron-Cohen}, Simon and Lai, Meng-Chuan and Mandy, William},
