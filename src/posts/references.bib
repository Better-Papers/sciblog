  title = {Spatiotemporal Patterns of Neuronal Subtype Genesis Suggest Hierarchical Development of Retinal Diversity},
  author = {West, Emma R. and Lapan, Sylvain W. and Lee, ChangHee and Kajderowicz, Kathrin M. and Li, Xihao and Cepko, Constance L.},
